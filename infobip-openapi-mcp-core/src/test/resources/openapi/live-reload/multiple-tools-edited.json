{
  "openapi": "3.1.0",
  "info": {
    "title": "Live Reload Test API",
    "version": "1.0.1"
  },
  "paths": {
    "/users": {
      "get": {
        "operationId": "getUsers",
        "summary": "List All Users",
        "description": "Get all users",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of users"
          }
        }
      }
    },
    "/products": {
      "get": {
        "operationId": "getProducts",
        "summary": "List Products",
        "description": "Get all products from catalog",
        "parameters": [
          {
            "name": "category",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of products"
          }
        }
      }
    },
    "/orders": {
      "get": {
        "operationId": "getOrders",
        "summary": "List Orders",
        "description": "Get all orders",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "customerId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of orders"
          }
        }
      }
    },
    "/items": {
      "get": {
        "operationId": "getItems",
        "summary": "List All Items",
        "description": "Get all items from inventory",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "inStock",
            "in": "query",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of items"
          }
        }
      }
    }
  }
}
